<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>$(AssemblyName)</Title>
    <Company>rjoseph</Company>
    <Authors>rjoseph</Authors>
    <Description>A library that provides convenient guard clause methods to check against things like nulls, whitespaces and defaults</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/rodnyjoseph200/Guarded</RepositoryUrl>
    <PackageTags>rjoseph;guarded;guard;checks;check;null;nullorwhitespace;default</PackageTags>
    <PackageReleaseNotes>A library that provides convenient guard clause methods to check against things like nulls, whitespaces and defaults</PackageReleaseNotes>
    <RepositoryType>git</RepositoryType>
    <RepositoryBranch>main</RepositoryBranch>
    <PackageTags>rjoseph;guarded;guard;checks;check;null;nullorwhitespace;default</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
